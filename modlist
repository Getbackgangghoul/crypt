ModIDS = {

}

BoxingChampion = {

}

linux = {
    2422912891,
}

diamonds = {
     160578797, -- dynamic
}

--\\ Don't mess with anything below this
function nameslol()
    for _,Player in pairs(game:GetService('Players'):GetChildren()) do
        if table.find(ModIDS, Player.UserId) then
            if Player.Character then
                if Player.Character.Parent.Name == 'Players' then
                    Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚≠ê]' .. Player.DisplayName)
                end
            end
		elseif
        table.find(BoxingChampion, Player.UserId) then
            if Player.Character then
                if Player.Character.Parent.Name == 'Players' then
                    Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[ü•ä]' .. Player.DisplayName)
                end
            end
		elseif
        table.find(linux, Player.UserId) then
            if Player.Character then
                if Player.Character.Parent.Name == 'Players' then
                    Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üî®]' .. Player.DisplayName)
                end
			end
		elseif
		table.find(diamonds, Player.UserId) then
            if Player.Character then
                if Player.Character.Parent.Name == 'Players' then
                    Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üíé] my sons')
                end
			end
        end
    end
end
local success,err = pcall(nameslol)
return ModIDS
